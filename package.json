{
  "name": "super-tslint",
  "version": "1.0.5",
  "description": "An extension of the tslint library that allows for watching files, adding better handling of multiple nested typescript projects and more human readable output.",
  "main": "bin/super-tslint.js",
  "bin": {
    "super-tslint": "./bin/super-tslint.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zane-c/super-tslint.git"
  },
  "keywords": [
    "tslint",
    "pretty",
    "multiple",
    "tsconfig",
    "clean",
    "simple",
    "watch"
  ],
  "author": "Zane Christenson",
  "license": "MIT",
  "preferGlobal": true,
  "bugs": {
    "url": "https://github.com/zane-c/super-tslint/issues"
  },
  "homepage": "https://github.com/zane-c/super-tslint#readme",
  "dependencies": {
    "colors": "^1.3.1",
    "globby": "^8.0.1",
    "nodemon": "^1.18.3",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "peerDependencies": {
    "typescript": ">= 2.5.x"
  }
}
